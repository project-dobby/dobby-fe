{
  "extends": "tslint-react",
  "rulesDirectory": [
    "src/"
  ],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "quotemark": [
      true,
      "single",
      "backtick"
    ],
    "no-arg": true,
    "new-parens": true,
    "no-bitwise": true,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "interface-name": [
      true,
      "never-prefix"
    ]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}